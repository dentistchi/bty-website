# JWT secret (production에서 반드시 변경)
JWT_SECRET=your-secret-here

# Gemini API (Safe Mirror 안전한 거울). 없으면 고정 폴백 응답 사용
GEMINI_API_KEY=

# OpenAI API (채팅 AI). Safe Mirror는 GEMINI_API_KEY 사용
OPENAI_API_KEY=

# 상대 사이트 URL (다른 사이트에서도 로그인 링크용)
NEXT_PUBLIC_TODAY_ME_URL=http://localhost:3000
NEXT_PUBLIC_BTY_URL=http://localhost:3001
