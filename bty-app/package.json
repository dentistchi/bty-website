{
  "name": "bty-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/check-env.mjs",
    "build": "npm run prebuild && next build",
    "build:local": "npm run prebuild && NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:fast": "npm run prebuild && SKIP_SOURCE_MAPS=1 NODE_OPTIONS='--max-old-space-size=4096' next build",
    "opennext:build": "opennextjs-cloudflare build",
    "deploy": "npm run prebuild && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "extract:questions": "node scripts/extract-questions.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:push": "supabase db push",
    "storage:cors": "bash scripts/storage-cors.sh"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.95.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.34.0",
    "jose": "^5.9.6",
    "next": "15.5.7",
    "next-auth": "^5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.5",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.5.7",
    "postcss": "^8.4.47",
    "supabase": "^2.76.15",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.0.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.64.0"
  },
  "engines": {
    "node": "20.x"
  }
}
