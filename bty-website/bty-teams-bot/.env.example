# Bot Framework (Azure Bot Service)
MicrosoftAppId=
MicrosoftAppPassword=

# bty-ai-core backend URL
BTY_BACKEND_URL=https://your-bty-ai-core.azurewebsites.net

# Role mapping via Microsoft Graph API (group membership)
# Add your Azure AD group object IDs. Requires App registration with
# Application permission: User.Read.All and GroupMember.Read.All
AZURE_AD_TENANT_ID=your-tenant-id
AZURE_AD_GROUP_LEADER=   # Object ID of Leadership group → role=leader
AZURE_AD_GROUP_DOCTOR=   # Object ID of Doctor group → role=doctor
